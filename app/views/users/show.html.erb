<center> 
    <h3> <%= @user.name %>'s Dashboard </h3>
    <p> <%= button_to "Discover Movies", "/users/#{@user.id}/discover", method: :get %>
    <br><br>
    <hr>
    <h3> Viewing Parties </h3>
    <%@user.viewing_parties.each do |party|%>
    <div id="viewing-party<%= party.id%>">
        <p><b><%= party.movie.title %></b></p>
    </div>
    <%end%>
</center>