<%= form_with model: User.new, local: true do |f| %>
  <%= f.label "Name:" %><br>
  <%= f.text_field :name %><br>
  <%= f.label "Email:" %><br>
  <%= f.text_field :email %><br><br>
  <%= f.label "Password:" %><br>
  <%= f.password_field :password %><br><br>
  <%= f.label "Password Confirmation:" %><br>
  <%= f.password_field :password_confirmation %><br><br>
  <%= f.submit "Create New User" %>
<% end %>